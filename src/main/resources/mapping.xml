<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
				"http://castor.org/mapping.dtd">
<mapping>
	<class name="sqlService.jaxb.Sqlmap">
		<map-to xml="sqlmap" />
		<field name="sql" type="sqlService.jaxb.SqlType" reqired="true" collection="arraylist">
			<bind-xml name="sql" node="element" />
		</field>
	</class>
	<class name="sqlService.jaxb.SqlType">
		<map-to xml="sqlmap" />
		<field name="key" type="string" reqired="true">
			<bind-xml name="sql" node="element" />
		</field>
		<field name="value" type="string" reqired="true">
			<bind-xml node="text" />
		</field>
	</class>
</mapping>